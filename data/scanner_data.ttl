@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix scn: <http://www.perspectum.com/resources/scanner/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

scn:URI_scn_ScannerManufacturer_Philips a scn:ScannerManufacturer,
        owl:NamedIndividual ;
    rdfs:label "Philips"^^xsd:string ;
    scn:isMakerOf scn:URI_scn_MRIScannerModel_Philips_ModelA,
        scn:URI_scn_MRIScannerModel_Philips_ModelB .

scn:URI_scn_ScannerManufacturer_Siemens a scn:ScannerManufacturer,
        owl:NamedIndividual ;
    rdfs:label "Siemens"^^xsd:string ;
    scn:isMakerOf scn:URI_scn_MRIScannerModel_Siemens_ModelA,
        scn:URI_scn_MRIScannerModel_Siemens_ModelB .

scn:URI_scn_MRIScannerModel_Philips_ModelA a scn:MRIScannerModel,
        owl:NamedIndividual ;
    rdfs:label "Philips_ModelA"^^xsd:string ;
    scn:FieldStrength 1.5e+00 ;
    scn:FieldStrengthUnit "Tesla"^^xsd:string .

scn:URI_scn_MRIScannerModel_Philips_ModelB a scn:MRIScannerModel,
        owl:NamedIndividual ;
    rdfs:label "Philips_ModelB"^^xsd:string ;
    scn:FieldStrength 5e+00 ;
    scn:FieldStrengthUnit "Tesla"^^xsd:string .

scn:URI_scn_MRIScannerModel_Siemens_ModelA a scn:MRIScannerModel,
        owl:NamedIndividual ;
    rdfs:label "Siemens_ModelA"^^xsd:string ;
    scn:FieldStrength 1.5e+00 ;
    scn:FieldStrengthUnit "Tesla"^^xsd:string .

scn:URI_scn_MRIScannerModel_Siemens_ModelB a scn:MRIScannerModel,
        owl:NamedIndividual ;
    rdfs:label "Siemens_ModelB"^^xsd:string ;
    scn:FieldStrength 3e+00 ;
    scn:FieldStrengthUnit "Tesla"^^xsd:string .

